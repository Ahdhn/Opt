#version 330 // -*- c++ -*-

/** 
\file imageDifference.pix 
*/
#include <compatibility.glsl>


uniform sampler2D input0_buffer;
uniform sampler2D input1_buffer;

out vec4 result;

void main() {
    result = texelFetch(input0_buffer, ivec2(gl_FragCoord.xy), 0) - texelFetch(input1_buffer, ivec2(gl_FragCoord.xy), 0);
}

